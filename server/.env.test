# Test environment configuration for E2E tests
# Note: E2E tests need database and Redis connections
NODE_ENV=test
PORT=3000

# Database configuration for testing
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=test
DB_DATABASE=medical_bible_test

# Redis configuration for testing
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=1

# JWT configuration
JWT_SECRET=test-jwt-secret-for-e2e-tests-only-do-not-use-in-production
JWT_ACCESS_EXPIRES=2h
JWT_REFRESH_EXPIRES=7d

# CORS configuration
CORS_ORIGIN=http://localhost:5173

# Encryption key
ENCRYPTION_KEY=test-encryption-key-32-characters-long
